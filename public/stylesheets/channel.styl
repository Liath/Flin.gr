@import 'nib'

#chat
  overflow-y:scroll
  padding:10px
  height:450px
#progress
  margin-bottom:0
  height:0px
  position: relative
#progress-text
  position: absolute
  z-index: 2
  text-align: center
  width: 100%
#file-progress
  margin-left: 0
#f
  display:none
#f-btn
  min-height:30px
  width:100%
#f-btn-box
  float:right
  margin:0
  width:8.33334%
  
.btn-primary:hover, .btn-primary:focus, .btn-primary:active, .btn-primary.active, .btn-primary.disabled, .btn-primary[disabled]
  color: #fff
  background-color: #1C140D
.btn-primary
  border:0
  text-align:center
  background-color: #006dcc
  background-repeat: repeat-x
  background-image: linear-gradient(top,lighten(#1C140D,15),#1C140D)
